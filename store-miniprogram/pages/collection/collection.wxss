/* pages/collection/collection.wxss */
page{
  background-color: #f5f5f5;
}
.empty {
  height: 100%;
  width: 100%;
  position: absolute;
  top: 0px;
  bottom: 0px;
  background-color: #f7f7f7;
}

.empty image {
  display: block;
  margin: 0 auto;
  padding-top: 50px;
  width: 147px;
  height: 136px;
}

.empty text {
  display: block;
  width: 100%;
  font-weight: 600;
  text-align: center;
  color: #b0b0b0;
  padding-top: 20px;
}