.list {
  width: 100%;
  background-color: #f5f5f5;
  overflow: hidden;
}

.list .list-main {
  margin: 2%;
  float: left;
}

/* .list .list-main::after {
  content: "";
  display: block;
  clear: left;
} */

.list .list-main .list-title {
  height: 50px;
  padding-left: 10px;
  line-height: 50px;
  font-size: 20px;
  color: #6c6c6c;
  font-weight: 550;
  background-color: #fff;
}

.list-main .pro-item {
  width: 49.5%;
  margin-top: 1%;
  float: left;
  background-color: #fff;
}

.list-main .pro-item:nth-child(2n) {
  margin-left: 1%;
}

.list-main .pro-item image {
  margin: 0 auto;
  padding: 0 20px 0 20px;
  width: 80%;
  height: 150px;
}

.list-main .pro-item .pro-info {
  margin: 0 10px 10px 10px;
}

.list-main .pro-item .pro-info .pro-title {
  width: 100%;
  height: 20px;
  float: left;
  font-size: 14px;
  font-weight: 400;
  color: #191919;
  text-overflow: ellipsis;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.list-main .pro-item .pro-info .pro-des {
  width: 100%;
  height: 14px;
  float: left;
  font-size: 12px;
  font-weight: 400;
  color: #7f7f7f;
  padding: 5px 0 5px 0;
  text-overflow: ellipsis;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.list-main .pro-item .pro-info .pro-price {
  width: 100%;
  height: 20px;
  padding-bottom: 10px;
  float: left;
  font-size: 16px;
  color: #ff6700;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.list-main .pro-item .pro-info .pro-price .del {
  font-size: 12px;
  height: 20px;
  padding-bottom: 10px;
  margin-left: 0.5em;
  color: #7f7f7f;
  text-decoration: line-through;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
